{
  "name": "hono-docker-k8s-example",
  "workspaces": ["apps/*"],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:web": "npm run dev -w web",
    "dev:api": "npm run dev -w api",
    "start": "run-p start:*",
    "start:web": "npm run start -w web",
    "start:api": "npm run start -w api",
    "build": "npm run build -ws",
    "clear": "npm run clear -ws"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
